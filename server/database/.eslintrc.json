{
  "env": {
    "es6": true,
    "node": true,
    "commonjs": true
  },
  "extends": ["eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": ["warn", { "args": "none" }],
    "no-console": "off",
    "indent": ["error", 2],
    "linebreak-style": "off",
    "quotes": ["warn", "single"],
    "semi": ["warn", "always"]
  }
}
